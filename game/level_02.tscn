script/source = "extends OmniLight3D

const MAX_ENERGY := 1.5
const MIN_TIME := 0.1
const MAX_TIME := 0.5

func _ready() -> void:
	var timer = Timer.new()
	timer.one_shot = true
	add_child(timer)
	while true:
		timer.start(randf_range(MIN_TIME, MAX_TIME))
		await timer.timeout
		light_energy = randf() * MAX_ENERGY
"

